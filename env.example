# Telegram Bot Configuration
# Get this from BotFather (https://t.me/BotFather)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here

# Admin Group ID
# This should be the Telegram chat ID of your admin group
# You can get this by adding @RawDataBot to your group
# and looking at the "chat" -> "id" field
ADMIN_GROUP_ID=-1001234567890

# Database Configuration
# Host where PostgreSQL server is running
DB_HOST=localhost
# PostgreSQL port
DB_PORT=5432
# Name of the database
DB_NAME=car_rental
# Username for PostgreSQL
DB_USER=postgres
# Password for PostgreSQL
DB_PASSWORD=your_password_here

# Logging Configuration
# Valid levels: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO